{
  "name": "wacanda",
  "private": true,
  "version": "1.0.0",
  "description": "Wacanda - RAG-powered AI Customer Service Agent",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run remove-debug-logs && tsc && vite build",
    "build:dev": "tsc && vite build",
    "remove-debug-logs": "node scripts/remove-debug-logs.js",
    "lint": "eslint . --max-warnings 100",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:performance": "echo 'Performance tests require k6 installation' && exit 0",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "vitest run --coverage && playwright test",
    "validate": "npm run lint && npm run test:ci && npm run build",
    "validate:phase6": "tsx scripts/validate-phase6.ts",
    "db:test": "supabase db test",
    "db:reset": "supabase db reset",
    "db:seed": "tsx scripts/seed-test-data.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cleanup-instances": "tsx scripts/cleanup-deleted-instances.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@supabase/supabase-js": "^2.38.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.0.0",
    "@whiskeysockets/baileys": "^6.7.18",
    "axios": "^1.10.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^2.8.0",
    "redis": "^5.5.6",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/blocks": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/react-vite": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.9.3",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-syntax-highlighter": "^15.5.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "globals": "^15.12.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.0.1",
    "msw": "^2.0.8",
    "node-fetch": "^2.7.0",
    "postcss": "^8.4.31",
    "storybook": "^7.6.3",
    "supabase": "^1.123.4",
    "tailwindcss": "^3.3.5",
    "tsx": "^4.4.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.34.0",
    "vite": "^4.5.0",
    "vitest": "^1.0.4",
    "wait-on": "^7.2.0"
  }
}
